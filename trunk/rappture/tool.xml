<?xml version="1.0"?>
<run>
  <tool>
    <title>Composite Filament Simulation 3D</title>
    <about>Description and credits</about>
    <command>
      python @tool/model.py @driver
    </command>
    <limits>
      <cputime>900</cputime>
    </limits>
    <action><label>Simulate</label></action>
    <layout>xxx</layout>
    <control>xxx</control>
    <uq>true</uq>
    <analyzer>xxx</analyzer>
    <reportJobFailures>1</reportJobFailures>
  </tool>
  <input>
    <number id="diameter">
      <about> <label>The diameter of the nanowires in microns</label> </about>
      <default>90.9e-3</default>
    </number>
    <number id="length">
      <about> <label>The length of the nanowires in microns</label> </about>
      <default>20</default>
    </number>
    <number id="volume_fraction">
      <about> <label>The volume fraction of nanowires</label> </about>
      <default>1</default>
    </number>
  </input>
  <output>
    <string id="path">
      <about> <label>Path found?</label></about>
    </string>
  </output>
</run>